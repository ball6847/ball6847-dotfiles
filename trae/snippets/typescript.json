{
	/* need _ to override builtin snippet */
	"_ Print to console": {
		"prefix": "l",
		"body": [
			"console.log($1)"
		],
		"description": "Log output to console"
	},
	"Insert comment line": {
		"prefix": "cc",
		"body": [
			"// -------------------------------------------------------------------"
		],
		"description": "Use as code block separator"
	},
	"Deno Test": {
		"prefix": [
			"dtest",
			"denotest"
		],
		"body": [
			"Deno.test(\"${1:test name}\", ${2:opts, }async () => {",
			"\t$0",
			"});"
		],
		"description": "Generate a Deno test block"
	}
}