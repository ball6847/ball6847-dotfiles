        "inversify",
        "jasminewd",
        "nestjs",
        "ng",
        "ngrx",
        "nrwl",
        "pregv",
        "readpref",
        "schematics",
        "scrollable",
        "Serializers",
        "signalr",
        "signalrjs",
        "store",
        "stylelint",
        "subcaption",
        "tabindex",
        "types",
        "utils",
        "virtuals",
        "zmotivat"
