        "kimi-k2-thinking": {
          "name": "Kimi K2 Thinking",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 262144,
            "output": 262144
          }
        }
      }
    },
    "ollama-v2": {
      "npm": "ollama-ai-provider-v2",
      "name": "Ollama Cloud v2",
      "options": {
        "baseURL": "https://ollama.com/api",
        "headers": {
          "Authorization": "Bearer {env:OLLAMA_API_KEY}"
        }
      },
      "models": {
        "deepseek-v3.1:671b": {
          "name": "DeepSeek-V3.1-671B",
          "limit": {
            "context": 160000,
            "output": 160000
          }
        },
        "gpt-oss:20b": {
          "name": "GPT-OSS-20B",
          "limit": {
            "context": 128000,
