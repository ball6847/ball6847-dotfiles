{
  "$schema": "https://opencode.ai/config.json",
  "theme": "opencode",
  "autoupdate": false,
  "instructions": [
    "AGENTS.md"
  ],
  "keybinds": {
    "leader": "ctrl+o"
  },
  "small_model": "ollama/gpt-oss:20b",
  "provider": {
    "ollama": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Ollama Cloud",
      "options": {
        "baseURL": "https://ollama.com/v1",
        "headers": {
          "Authorization": "Bearer {env:OLLAMA_API_KEY}"
        }
      },
      "models": {
        "deepseek-v3.1:671b": {
          "name": "DeepSeek-V3.1-671B",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 160000,
            "output": 160000
          }
        },
        "gpt-oss:20b": {
          "name": "GPT-OSS-20B",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 128000,
            "output": 128000
          }
        },
        "gpt-oss:120b": {
          "name": "GPT-OSS-120B",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 128000,
            "output": 128000
          }
        },
        "kimi-k2:1t": {
          "name": "Kimi-K2-1T",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 256000,
            "output": 256000
          }
        },
        "qwen3-coder:480b": {
          "name": "Qwen3-Coder-480B",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 256000,
            "output": 256000
          }
        },
        "glm-4.6": {
          "name": "GLM-4.6",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
