        },
        "minimax-m2": {
          "name": "MiniMax-M2",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 200000,
            "output": 200000
