{
  "$schema": "https://opencode.ai/config.json",
  "theme": "opencode",
  "autoupdate": false,
  "instructions": ["AGENTS.md"],
  "keybinds": {
    "leader": "ctrl+o"
  },
  "plugin": ["opencode-gemini-auth", "opencode-alibaba-qwen3-auth"],
  "small_model": "openrouter/google/gemini-2.0-flash-exp:free",
  "tui": {
    "scroll_speed": 3
  },
  "provider": {
    "ollama": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Ollama Cloud",
      "options": {
        "baseURL": "https://ollama.com/v1",
        "headers": {
          "Authorization": "Bearer {env:OLLAMA_API_KEY}"
        }
      },
      "models": {
        "deepseek-v3.1:671b": {
          "name": "DeepSeek-V3.1-671B",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 160000,
            "output": 160000
          }
        },
        "gpt-oss:20b": {
          "name": "GPT-OSS-20B",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 128000,
            "output": 128000
          }
        },
        "gpt-oss:120b": {
          "name": "GPT-OSS-120B",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 128000,
            "output": 128000
          }
        },
        "kimi-k2:1t": {
          "name": "Kimi-K2-1T",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 256000,
            "output": 256000
          }
        },
        "qwen3-coder:480b": {
          "name": "Qwen3-Coder-480B",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 256000,
            "output": 256000
          }
        },
        "glm-4.6": {
          "name": "GLM-4.6",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 198000,
            "output": 198000
          }
        },
        "minimax-m2": {
          "name": "MiniMax-M2",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 200000,
            "output": 200000
          }
        },
        "kimi-k2-thinking": {
          "name": "Kimi K2 Thinking",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "options": {
            "reasoningEffort": "none",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 262144,
            "output": 262144
          }
        }
      }
    },
    "kimi-for-coding": {
      "npm": "@ai-sdk/anthropic",
      "name": "Kimi For Coding",
      "options": {
        "baseURL": "https://api.kimi.com/coding/v1",
        "apiKey": "{env:KIMI_API_KEY}"
      },
      "models": {
        "kimi-for-coding": {
          "name": "Kimi For Coding",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 262144,
            "output": 32768
          }
        }
      }
    },
    "vanchin": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Vanchin KAT-Coder",
      "options": {
        "baseURL": "https://vanchin.streamlake.ai/api/gateway/v1/endpoints",
        "headers": {
          "Authorization": "Bearer {env:VC_API_KEY}"
        }
      },
      "models": {
        "ep-y5jyym-1762763475872074194": {
          "name": "KAT-Coder-Pro",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 200000,
            "output": 200000
          }
        },
        "ep-trsvc7-1762763786443153146": {
          "name": "KAT-Coder-Air",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 200000,
            "output": 200000
          }
        }
      }
    },
    "zenmux-custom": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "zenmux-custom",
      "options": {
        "baseURL": "https://zenmux.ai/api/v1",
        "headers": {
          "Authorization": "Bearer {env:ZENMUX_API_KEY}"
        }
      },
      "models": {
        "deepseek/deepseek-reasoner": {
          "name": "DeepSeek-V3.2-Exp (Thinking Mode)",
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 128000,
            "output": 64000
          }
        },
        "openai/gpt-5-nano": {
          "name": "GPT-5 Nano",
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 400000,
            "output": 128000
          }
        },
        "volcengine/doubao-seed-code": {
          "name": "Doubao-Seed-Code",
          "options": {
            "reasoningEffort": "low",
            "textVerbosity": "low"
          },
          "limit": {
            "context": 256000,
            "output": 32000
          }
        }
      }
    }
  },
  "mcp": {}
}
