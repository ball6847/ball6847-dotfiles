      "name": "Kimi For Coding",
      "options": {
        "baseURL": "https://api.kimi.com/coding/v1",
        "apiKey": "{env:KIMI_API_KEY}"
      },
      "models": {
        "kimi-for-coding": {
          "name": "Kimi For Coding",
          "reasoning": false,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 262144,
            "output": 32768
          }
        }
      }
    }
  },
  "mcp": {
    "jina": {
      "type": "remote",
      "url": "https://mcp.jina.ai/sse",
      "enabled": true,
      "headers": {
        "Authorization": "Bearer {env:JINA_API_KEY}"
      }
    }
  }
