{
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": false,
  "permission": "allow",
  "instructions": ["AGENTS.md"],
  "plugin": [
    // "opencode-gemini-auth",
    // "opencode-alibaba-qwen3-auth",
    // "opencode-skills",
    // "opencode-orchestrator",
    "opencode-scheduler"
  ],
  "small_model": "mistral/labs-devstral-small-2512",
  "provider": {
    "kimi-for-coding": {
      "npm": "@ai-sdk/anthropic",
      "name": "Kimi For Coding",
      "options": {
        "baseURL": "https://api.kimi.com/coding/v1",
        "apiKey": "{env:KIMI_API_KEY}"
      },
      "models": {
        "kimi-for-coding": {
          "name": "Kimi For Coding",
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 262144,
            "output": 32768
          }
        }
      }
    },
    "synthetic": {
      "id": "synthetic",
      "env": ["SYNTHETIC_API_KEY"],
      "npm": "@ai-sdk/openai-compatible",
      "api": "https://api.synthetic.new/v1",
      "name": "Synthetic",
      "models": {
        "hf:Qwen/Qwen3.5-397B-A17B": {
          "id": "hf:Qwen/Qwen3.5-397B-A17B",
          "name": "Qwen 3.5 397B A17B",
          "family": "qwen",
          "attachment": true,
          "reasoning": true,
          "tool_call": true,
          "temperature": true,
          "release_date": "2026-02-19",
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          },
          "cost": {
            "input": 0.6,
            "output": 3
          },
          "limit": {
            "context": 262144,
            "output": 65536
          }
        },
        "hf:MiniMaxAI/MiniMax-M2.5": {
          "id": "hf:MiniMaxAI/MiniMax-M2.5",
          "name": "MiniMax-M2.5",
          "family": "minimax",
          "attachment": true,
          "reasoning": true,
          "tool_call": true,
          "temperature": true,
          "release_date": "2026-02-15",
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          },
          "cost": {
            "input": 0.6,
            "output": 3
          },
          "limit": {
            "context": 191488,
            "output": 65536
          }
        }
      }
    },
    "byteplus": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "byteplus",
      "options": {
        "baseURL": "https://ark.ap-southeast.bytepluses.com/api/coding/v3",
        "apiKey": "{env:ARK_API_KEY}"
      },
      "models": {
        "bytedance-seed-code": {
          "name": "bytedance-seed-code"
        },
        "kimi-k2.5": {
          "name": "kimi-k2.5"
        },
        "glm-4.7": {
          "name": "glm-4.7"
        },
        "kimi-k2-thinking": {
          "name": "kimi-k2-thinking"
        },
        "gpt-oss-120b": {
          "name": "gpt-oss-120b"
        }
      }
    }
  },
  "mcp": {
    "openmemory": {
      "type": "remote",
      "url": "{env:OPENMEMORY_API_URL}",
      "headers": {
        "Authorization": "Bearer {env:OPENMEMORY_API_KEY}"
      }
    }
  }
}
